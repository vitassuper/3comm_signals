{
  "indicators": [
      {
          "name": "ema",
          "timeframe": "15min",
          "parameters": {
              "length": 100,
              "offset": 0
          }
      },
      {
          "name": "bbands",
          "timeframe": "5min",
          "parameters": {
              "length": 130,
              "ma_type": "ema",
              "std_dev": 1.2
          }
      },
    {
      "name": "candlesticks",
      "timeframe": "1min"
    }
  ],
  "new_long": "candlesticks.open < bbands.upper and (candlesticks.close > bbands.upper or bbands.middle > ema)",
  "rules": {
    "long": {
      "type": "crossover",
      "indicator1": "bbands.upper",
      "indicator2": "ema",
      "direction": "top_to_bottom"
    }
  }
}